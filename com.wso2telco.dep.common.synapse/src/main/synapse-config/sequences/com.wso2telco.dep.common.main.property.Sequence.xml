<?xml version="1.0" encoding="UTF-8"?>
<sequence name="com.wso2telco.dep.common.main.property.Sequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
  <property expression="$trp:CONSUMER_KEY" name="CONSUMER_KEY"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:USER_ID" name="USER_ID" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:CONTEXT" name="CONTEXT" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:API_VERSION" name="API_VERSION"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:API_NAME" name="API_NAME" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:API_ID" name="API_ID" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:VERSION" name="VERSION" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:RESOURCE" name="RESOURCE" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:HTTP_METHOD" name="HTTP_METHOD"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:HOST_NAME" name="HOST_NAME" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:API_PUBLISHER" name="API_PUBLISHER"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:APPLICATION_NAME" name="APPLICATION_NAME"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:APPLICATION_ID" name="APPLICATION_ID"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:operator" name="OPERATOR" scope="default"
    type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <property expression="$trp:REQUEST_ID" name="REQUEST_ID"
    scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
  <filter xmlns:ns="http://org.apache.synapse/xsd" xpath="boolean($trp:X-JWT-Assertion)">
    <then/>
    <else>
      <filter xpath="boolean($trp:Authorization)">
        <then>
          <header expression="$trp:Authorization"
                  name="X-JWT-Assertion" scope="transport"/>
          <property name="AuthorizationHeader" scope="default" value="true"/>
        </then>
        <else>
          <property name="AuthorizationHeader" scope="default" value="false"/>
        </else>
      </filter>
    </else>
  </filter>
</sequence>
