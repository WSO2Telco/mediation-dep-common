	<msisdn_regex_num_grp>9</msisdn_regex_num_grp>
	<payload_logging_enabled>false</payload_logging_enabled>
	<validationConfig>
		<requestIdentifierLength>6</requestIdentifierLength>
	</validationConfig>
</mediationConfig>